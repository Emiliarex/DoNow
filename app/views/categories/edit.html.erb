<h1>Edit Your Category!</h1>
<div class="col-md-8">
  <%= form_for @category do |f| %>
  <div class="form-group">
  <%= f.label :title %>
  <%= f.text_field :title, class: 'form-control', placeholder: "Put Your Amazing Category Name Here" %>
    <%= f.submit "Edit Category", class: 'btn btn-warning' %>
  
  <%end %>
  </div>
</div>

